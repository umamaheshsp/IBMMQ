if   [ "$HOST" = "hpux23" ]
then
   hpux23
elif [ "$HOST" = "hpux24" ]
then
   hpux24
elif [ "$HOST" = "hpux84" ]
then
   hpux84
elif [ "$HOST" = "hpux25" ]
then
   hpux25
else
   echo "This script cannot be run on $HOST"
   echo " "
   exit
fi
