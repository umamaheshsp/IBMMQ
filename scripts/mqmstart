#!/sbin/sh   
#
# MQ startup
#
HOST=`hostname`
PATH=/sbin:/usr/sbin:/usr/bin
export PATH

hpux23()
{
	echo "Starting QMGR SCPMQSCM23"
	/usr/bin/strmqm SCPMQSCM23
	#echo "Starting Command Server for SCPMQSCM23"
   	#/usr/bin/strmqcsv SCPMQSCM23

	#echo "Starting QMGR SCPMQSWT23"
	#/usr/bin/strmqm SCPMQSWT23
	#echo "Starting Command Server for SCPMQSWT23"
   	#/usr/bin/strmqcsv SCPMQSWT23

	echo "Starting QMGR SCPMQVEN23"
	/usr/bin/strmqm SCPMQVEN23
	#echo "Starting Command Server for SCPMQVEN23"
   	#/usr/bin/strmqcsv SCPMQVEN23

	echo "Starting QMGR SCPMQDCA23"
	/usr/bin/strmqm SCPMQDCA23
	#echo "Starting Command Server for SCPMQDCA23"
   	#/usr/bin/strmqcsv SCPMQDCA23

	echo "Starting QMGR SCPMQARC23"
	/usr/bin/strmqm SCPMQARC23
	#echo "Starting Command Server for SCPMQARC23"
   	#/usr/bin/strmqcsv SCPMQARC23

	echo "Starting QMGR SCPMQABU23"
	/usr/bin/strmqm SCPMQABU23
	#echo "Starting Command Server for SCPMQABU23"
   	#/usr/bin/strmqcsv SCPMQABU23

	echo "Starting QMGR SCPMQMSG23"
	/usr/bin/strmqm SCPMQMSG23
	#echo "Starting Command Server for SCPMQMSG23"
   	#/usr/bin/strmqcsv SCPMQMSG23

}

